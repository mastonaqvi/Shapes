#include <iostream>
using namespace std;

int main() {
    int row, spc, col;

    // Upper part of the hollow diamond
    for (row = 1; row <= 4; row++) {
        for (spc = 1; spc <= 4 - row; spc++) {
            cout << " ";
        }
        for (col = 1; col <= row * 2 - 1; col++) {
            if (col == 1 || col == row * 2 - 1) {
                cout << "*";
            } else {
                cout << " ";
            }
        }
        cout << endl;
    }

    // Lower part of the hollow diamond
    for (row = 4 - 1; row >= 1; row--) {
        for (spc = 1; spc <= 4 - row; spc++) {
            cout << " ";
        }
        for (col = 1; col <= row * 2 - 1; col++) {
            if (col == 1 || col == row * 2 - 1) {
                cout << "*";
            } else {
                cout << " ";
            }
        }
        cout << endl;
    }

    return 0;
}
